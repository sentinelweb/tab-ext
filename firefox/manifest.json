{
  "manifest_version": 2,
  "name": "Tab Saver",
  "description": "Save and restore your tabs",
  "version": "1.0",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "tabs.png"
  },
  "browser_action": {
    "default_icon": "tabs.png",
    "default_popup": "popup.html"
  }
}
